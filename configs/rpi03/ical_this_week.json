{
    "module": "ical",
    "moddef": {
        "name": "this_week",
        "position": "icalendar_this_week",
        "text_color": "#fff",
        "text_bg_color": "#000",
        "font_size": 24,
        "disabled": false,
        "force_render": false
    },
    "ical": {
        "calendar_name": "gregs_calendar",
        "title": "This Week",
        "time_format": "Sun 0:00",
        "number_days": 14,
        "show_regular_events": true,
        "show_all_day_events": false,
        "show_recurring_events": true
    },
    "card": {
        "header": {
            "font_name": "Roboto-Regular",
            "font_size": 32,
            "text_color": "#fff",
            "text_bg_color": "gray",
            "height": 72
        },
        "body": {
            "font_name": "Roboto-Regular",
            "font_size": 40,
            "text_color": "#0ff",
            "text_bg_color": "#000",
            "valign": "center",
            "halign": "left",
            "wrap": "none"
        },
        "footer": {
            "font_name": "Roboto-Regular",
            "font_size": 32,
            "text_color": "#fff",
            "text_bg_color": "gray",
            "valign": "bottom",
            "height": 72
        }
    }
}